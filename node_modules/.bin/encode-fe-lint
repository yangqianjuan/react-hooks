#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/encode-fe-lint@1.0.3_typescript@5.2.2/node_modules/encode-fe-lint/lib/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/encode-fe-lint@1.0.3_typescript@5.2.2/node_modules/encode-fe-lint/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/encode-fe-lint@1.0.3_typescript@5.2.2/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/encode-fe-lint@1.0.3_typescript@5.2.2/node_modules/encode-fe-lint/lib/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/encode-fe-lint@1.0.3_typescript@5.2.2/node_modules/encode-fe-lint/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/encode-fe-lint@1.0.3_typescript@5.2.2/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../encode-fe-lint/lib/cli.js" "$@"
else
  exec node  "$basedir/../encode-fe-lint/lib/cli.js" "$@"
fi
