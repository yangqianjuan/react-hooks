#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/r2.cnpmjs.org+surge@0.21.3/node_modules/surge/lib/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/r2.cnpmjs.org+surge@0.21.3/node_modules/surge/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/r2.cnpmjs.org+surge@0.21.3/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/r2.cnpmjs.org+surge@0.21.3/node_modules/surge/lib/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/r2.cnpmjs.org+surge@0.21.3/node_modules/surge/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/r2.cnpmjs.org+surge@0.21.3/node_modules:/Users/yangqianlu/Desktop/hooks实战练习/reactDemoHooks/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../surge/lib/cli.js" "$@"
else
  exec node  "$basedir/../surge/lib/cli.js" "$@"
fi
